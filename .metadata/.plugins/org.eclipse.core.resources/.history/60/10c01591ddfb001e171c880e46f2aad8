#EUREKA SERVER CONFIGURATION
eureka:
  client:
    fetch-registry: true
    register-with-eureka: true
    service-url:
      defaultZone: http://eureka:password@eureka-server-test:8761/eureka
  instance:
    prefer-ip-address: true
    
server:
  port: 7771
  
spring:
  application:
    name: AUTHENTICATION-SERVICE
  profiles:
    active: docker