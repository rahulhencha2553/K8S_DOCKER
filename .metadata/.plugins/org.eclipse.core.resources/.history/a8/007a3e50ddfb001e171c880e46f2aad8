#EUREKA SERVER CONFIGURATION
eureka:
  client:
    fetch-registry: true
    register-with-eureka: true
    service-url:
      defaultZone: http://localhost:8761/eureka
  instance:
    prefer-ip-address: true
    
spring:
  application:
    name: API-GATEWAY
  profiles:
    active: docker

server:
  port: 9090